#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/vite@7.0.4/node_modules/vite/bin/node_modules:/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/vite@7.0.4/node_modules/vite/node_modules:/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/vite@7.0.4/node_modules:/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/vite@7.0.4/node_modules/vite/bin/node_modules:/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/vite@7.0.4/node_modules/vite/node_modules:/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/vite@7.0.4/node_modules:/mnt/c/Users/User/00-PROGRAMACION/01-FULLSTACK-ENGINER/05_FULLSTACK_PATH_SCRIMB/01_CSS_HOMETOWN_HOMEPAGE_150725/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
